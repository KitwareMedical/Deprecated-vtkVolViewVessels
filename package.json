        "paraviewweb": "2.5.6",
        "daikon": "1.2.17"
    },
    "devDependencies": {
        "font-awesome": "4.5.0",
        "normalize.css": "4.1.1",
        "kw-doc": "1.1.1",
        "kw-web-suite": "3.1.1",
        "monologue.js": "0.3.5",
        "mout": "1.0.0",
        "react": "15.3.1",
        "react-dom": "15.3.1",
        "d3": "3.5.17",
        "axios": "0.14.0",
        "gl-matrix": "2.3.1",
        "hammerjs": "2.0.8",
        "babel-polyfill": "6.16.0"
    },
    "scripts": {
        "start": "webpack-dev-server",
