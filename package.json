{
    "name": "tube",
    "version": "0.0.0-semantically-release",
    "description": "Medical Web Viewer",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kitware/slicer-web.git"
    },
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/kitware/slicer-web/issues"
    },
